<configuration>
  <configSections>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <!-- Enable config builder for loading xml and json secrets -->
    <section name="configBuilders" type="System.Configuration.ConfigurationBuildersSection, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" restartOnExternalChanges="false" requirePermission="false" />
  </configSections>
    
  <configBuilders>
    <builders>
      <!-- An xml file which contains sensitive data and is not stored in the repo. Our config builder will read it and pull the values into our app settings at run time. -->
      <add name="UserSecrets" mode="Greedy" prefix="AppSetting_" userSecretsFile="~/../../secrets.debug/usersecrets.xml" stripPrefix="true" type="Microsoft.Configuration.ConfigurationBuilders.UserSecretsConfigBuilder, Microsoft.Configuration.ConfigurationBuilders.UserSecrets, Version=3.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
      <!-- A json file which contains sensitive data and is not stored in the repo. Our config builder will read it and pull the values into our app settings at run time.
      IMPORTANT - see https://github.com/aspnet/MicrosoftConfigurationBuilders/blob/main/samples/SampleWebApp/App_Data/settings.json -->
      <add name="Json" mode="Greedy" jsonMode="Sectional" jsonFile="~/../../secrets.debug/seed.json" type="Microsoft.Configuration.ConfigurationBuilders.SimpleJsonConfigBuilder, Microsoft.Configuration.ConfigurationBuilders.Json" />
      <!-- Also pull the connection strings in from a JSON file. They are not stored in the repo, for security reasons. -->
      <add name="ConnectionStringsJson" mode="Greedy" jsonMode="Sectional" jsonFile="~/../../secrets.debug/connections.json" type="Microsoft.Configuration.ConfigurationBuilders.SimpleJsonConfigBuilder, Microsoft.Configuration.ConfigurationBuilders.Json" />
    </builders>
  </configBuilders>
    
  <appSettings configBuilders="UserSecrets,Json">
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <!--
    Tell the framework to read our validation rules straight off the data annotations of viewmodel
    and then automatically use them with unobtrusive jquery validation.
    Note - The Unobtrusive Validation plugin automatically constructs and calls the .validate() method
    based on the various data attributes in your form. jQuery Validate plugin does not allow .validate()
    to be called more than once on the same form. So any second call to .validate() like from within my
    page javascript, will be totally ignored.
    See    https://kitsula.com/Article/ASP-NET-MVC-3-JQuery-validation-Data-Annotation-Unobtrusive-Client-Validation
    See    https://stackoverflow.com/questions/39255135/jquery-validate-is-not-executing-while-using-unobtrusive-validation
    -->
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- Dont send emails, write them to a text file-->
    <add key="Email.WriteAsFile" value="true" />
    <!-- Tell OWIN where our startup class is located-->
    <add key="owin:AppStartup" value="App472.WebUI.OwinIdentityConfig" />
    <!-- Define where our DBContext class(es) should look, for their database initializer(s) -->
    <!-- Initializer for this class (LEFT) in this assembly (LEFT), is found in this class (RIGHT) in this assembly (RIGHT) -->
    <add key="DatabaseInitializerForType App472.WebUI.Models.IDDBContext, App472.WebUI" value="App472.WebUI.App_Start.Debug.IDDBInitializer, App472.WebUI" />
    <add key="DatabaseInitializerForType App472.Domain.Concrete.EFDBContext, App472.Domain" value="App472.WebUI.App_Start.Debug.EFDBInitializer, App472.WebUI" />
  </appSettings>

  <!-- The config builder will insert our connection string values. They are not stored in the repo, for security reasons. -->
  <connectionStrings configBuilders="ConnectionStringsJson" />
    
  <system.web>
    <httpRuntime targetFramework="4.7.2" />
    <!-- Set compilation debug="false" to enable bundling and minification -->
    <compilation debug="true" targetFramework="4.7.2" />
    <!--
    Below I have turned off embedded and automatic security modules of ASP.NET so that OAuth security module can step in.
    See https://jeffreyfritz.com/2014/10/2014105aspnet-identity-oauth-and-loginaspx/
    -->
    <authentication mode="None" />
    <membership>
      <providers>
        <clear />
      </providers>
    </membership>
    <profile>
      <providers>
        <clear />
      </providers>
    </profile>
    <roleManager>
      <providers>
        <clear />
      </providers>
    </roleManager>
  </system.web>
  <!--
  FormsAuthentication module is disabled by default for applications that use ASP.NET identity and OAuth security.
  See https://jeffreyfritz.com/2014/10/2014105aspnet-identity-oauth-and-loginaspx/
  -->
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>
  </system.webServer>
